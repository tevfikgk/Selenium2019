package com.class3;

public class ShorthandOperators {
	public  static void main (String [] arg) {
		
		int num4=50;
		num4+=20;
		System.out.println(num4);		
				
		num4-=50;
		System.out.println(num4);
		
		num4/=30;
		System.out.println(num4);
		
	    num4%=30;
		System.out.println(num4);
		
		num4*=40;
		System.out.println(num4);
		
	}

}
