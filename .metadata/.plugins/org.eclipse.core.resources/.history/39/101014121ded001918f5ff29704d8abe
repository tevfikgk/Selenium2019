package com.class7;

public class DoWhile {

	
	
	public static void main(String[] args) {
		
		int num=1;
		while(num<=7){
			System.out.println("Hello");
			num++;
		}
		int num1=10;
		do {
			System.out.println("Bye");
			num++;
		}while (num1<=7);{
		System.out.println("..................");
		}num1++;
		
	}
}
