package com.class23;

public class TestClass {

	public static String patientName;
	public double length;
	public int weight;
	
	
	TestClass(){
	
	}
	TestClass(double patientLength ,int patientWeight)
	{
	length=patientLength;
	weight = patientWeight;
	}
	public void situation() {
	System.out.println("Patient name "+patientName+" patient length: "+length+ " feet"+ " patient weigth:"+weight+" pound");
	}

	public static void main(String[] args) {
		String patientName="Ali Usta";
		TestClass str1=new TestClass(5.7,185);
		str1.situation();
	}
}
